{
    "name": "endbot",
    "description": "Minecraft linking bot with other general utilities.",
    "version": "5.0.0",
    "private": true,
    "main": "dist/src/index.js",
    "type": "module",
    "author": "samipourquoi",
    "contributors": [
        "trunkyredstone",
        "iDarkLightning",
        "jezzerswastaken",
        "londonmine",
        "syntro"
    ],
    "repository": "git+https://github.com/samipourquoi/endbot.git",
    "homepage": "https://github.com/samipourquoi/endbot#readme",
    "bugs": {
        "url": "https://github.com/samipourquoi/endbot/issues"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "dev": "tsc-watch --onSuccess \"yarn start\"",
        "format": "prettier --write \"**/*.ts\" --cache",
        "lint": "eslint .",
        "lint-fix": "yarn lint --fix",
        "start": "node --disable-warning=ExperimentalWarning .",
        "test": "jest --config=tests/jest.config.js"
