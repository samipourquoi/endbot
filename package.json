    "devDependencies": {
        "@babel/core": "^7.23.6",
        "@babel/preset-env": "^7.23.6",
        "@babel/preset-typescript": "^7.23.3",
        "@swc/core": "^1.3.101",
        "@swc/jest": "^0.2.29",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "@types/tail": "^2.2.3",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "babel-jest": "^29.7.0",
        "eslint": "^8.56.0",
        "eslint-plugin-prettier": "^5.1.1",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "ts-node": "^10.9.2",
        "tsc-watch": "^6.0.4"
    }
